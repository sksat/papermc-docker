{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": [".env"],
      "matchStrings": ["depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?\\s.*?_COMMIT=(?<currentValue>.*)\\s"],
      "versioningTemplate": "git",
      "dataSourceTemplate": "git-ref"
    }
  ]
}
